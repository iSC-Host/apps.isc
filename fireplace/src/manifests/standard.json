{
  "short_name": "Marketplace",
  "name": "Firefox Marketplace",
  "icons": [
    {
      "src": "/media/img/app-icons/60.png",
      "sizes": "60x60",
      "type": "image/png",
      "density": "1.0"
    },
    {
      "src": "/media/img/app-icons/90.png",
      "sizes": "90x90",
      "type": "image/png",
      "density": "1.5"
    },
    {
      "src": "/media/img/app-icons/120.png",
      "sizes": "120x120",
      "type": "image/png",
      "density": "2.0"
    },
    {
      "src": "/media/img/app-icons/128.png",
      "sizes": "128x128",
      "type": "image/png",
      "density": "3.0"
    }
  ],
  "start_url": "/",
  "display": "standalone",
  "orientation": "portrait-primary",
  "moz_locales": ["bg", "bn-BD", "ca", "cs", "de", "el", "en-US", "es", "eu", "fr", "ga-IE", "hr", "hu", "it", "ja", "ko", "mk", "nb-NO", "nl", "pa", "pl", "pt-BR", "ro", "ru", "sk", "sq", "sr", "sr-Latn", "ta", "tr", "xh", "zh-CN", "zh-TW", "zu"],
  "moz_permissions": {
    "mobilenetwork": {
      "description": "To detect mobile carrier and regional information."
    },
    "moz-firefox-accounts": {
      "description": "To use native (2.0+) Firefox Accounts login."
    },
    "tcp-socket": {
      "description": "To detect online status."
    }
  },
  "moz_version": "{version}"
}
